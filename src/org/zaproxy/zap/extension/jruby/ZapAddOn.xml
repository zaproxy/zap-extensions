<zapaddon>
	<name>Ruby scripting</name>
	<version>6</version>
	<status>beta</status>
	<description>Allows Ruby to be used for ZAP scripting - templates included</description>
	<author>ZAP Dev Team</author>
	<url></url>
	<changes>
	<![CDATA[
	Updated for 2.7.0.<br>
	]]>
	</changes>
	<extensions>
	    <extension>org.zaproxy.zap.extension.jruby.ExtensionJruby</extension>
	</extensions>
	<ascanrules/>
	<pscanrules/>
	<files>
		<file>scripts/templates/active/Active default template.rb</file>
		<file>scripts/templates/authentication/Authentication default template.rb</file>
		<file>scripts/templates/authentication/Simple Form-Based Authentication.rb</file>
		<file>scripts/templates/httpsender/HttpSender default template.rb</file>
		<file>scripts/templates/passive/Passive default template.rb</file>
		<file>scripts/templates/proxy/Proxy default template.rb</file>
		<file>scripts/templates/standalone/Loop through history table.rb</file>
		<file>scripts/templates/standalone/Standalone default template.rb</file>
		<file>scripts/templates/standalone/Traverse sites tree.rb</file>
		<file>scripts/templates/targeted/Targeted default template.rb</file>
	</files>
	<not-before-version>2.7.0</not-before-version>
	<not-from-version/>
</zapaddon>
