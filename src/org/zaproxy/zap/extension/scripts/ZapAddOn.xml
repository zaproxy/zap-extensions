<zapaddon>
	<name>Script Console</name>
	<version>25</version>
	<status>beta</status>
	<description>Supports all JSR 223 scripting languages</description>
	<author>ZAP Dev Team</author>
	<url>https://github.com/zaproxy/zaproxy/wiki/ScriptConsole</url>
	<changes>
	<![CDATA[
	Fix typo in help page.<br>
	Execute Targeted scripts in other thread than GUI thread.<br>
	Clear highlighting syntax when a non-script node is selected.<br>
	Warn of script changed by another program (post 2.7.0).<br>
	]]>
	</changes>
	<extensions>
		<extension>org.zaproxy.zap.extension.scripts.ExtensionScriptsUI</extension>
	</extensions>
	<ascanrules/>
	<pscanrules/>
	<filters/>
	<files>
		<file>scripts/templates/extender/Add api call.js</file>
		<file>scripts/templates/extender/Add history record menu.js</file>
		<file>scripts/templates/extender/Add toolbar button.js</file>
		<file>scripts/templates/extender/Add panels.js</file>
		<file>scripts/templates/extender/Add tools menu.js</file>
		<file>scripts/templates/extender/Copy as curl command menu.js</file>
		<file>scripts/templates/extender/Extender default template.js</file>
	</files>
	<not-before-version>2.7.0</not-before-version>
	<not-from-version/>
</zapaddon>
