<zapaddon>
	<name>Python Scripting</name>
	<version>11</version>
	<status>beta</status>
	<description>Allows Python to be used for ZAP scripting - templates included</description>
	<author>ZAP Dev Team</author>
	<url></url>
	<changes>
	<![CDATA[
	Update the help to mention the bundled Jython version.<br>
	Jython templates now includes an extender script (getInputsFromuser.py) for setting global script variables based on user input.<br> 
	]]>
	</changes>
	<extensions>
	    <extension>org.zaproxy.zap.extension.jython.ExtensionJython</extension>
	</extensions>
	<ascanrules/>
	<pscanrules/>
	<files>
		<file>scripts/templates/active/Active default template.py</file>
		<file>scripts/templates/authentication/Authentication default template.py</file>
		<file>scripts/templates/authentication/Simple Form-Based Authentication.py</file>
		<file>scripts/templates/extender/Add tools menu.py</file>
		<file>scripts/templates/extender/Extender default template.py</file>
		<file>scripts/templates/httpsender/HttpSender default template.py</file>
		<file>scripts/templates/passive/Passive default template.py</file>
		<file>scripts/templates/proxy/Proxy default template.py</file>
		<file>scripts/templates/standalone/Loop through history table.py</file>
		<file>scripts/templates/standalone/Standalone default template.py</file>
		<file>scripts/templates/standalone/Traverse sites tree.py</file>
		<file>scripts/templates/targeted/Targeted default template.py</file>
		<file>scripts/templates/variant/Input Vector default template.py</file>
	</files>
	<not-before-version>2.7.0</not-before-version>
	<not-from-version/>
</zapaddon>
