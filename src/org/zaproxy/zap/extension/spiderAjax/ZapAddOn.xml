<zapaddon>
	<name>Ajax Spider</name>
	<version>23</version>
	<status>release</status>
	<description>Allows you to spider sites that make heavy use of JavaScript using Crawljax</description>
	<author>ZAP Dev Team</author>
	<url>https://github.com/zaproxy/zap-core-help/wiki/HelpAddonsSpiderAjaxConcepts</url>
	<changes>
	<![CDATA[
	Correct WebDriver requester ID.<br>
	]]>
	</changes>
	<dependencies>
		<addons>
			<addon>
				<id>selenium</id>
				<semver>2.*</semver>
			</addon>
		</addons>
	</dependencies>
	<extensions>
		<extension>org.zaproxy.zap.extension.spiderAjax.ExtensionAjax</extension>
	</extensions>
	<ascanrules></ascanrules>
	<pscanrules/>
	<files/>
	<not-before-version>2.7.0</not-before-version>
	<not-from-version/>
</zapaddon>

