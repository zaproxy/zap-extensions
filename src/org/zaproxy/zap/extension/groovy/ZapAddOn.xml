		<file>scripts/templates/passive/PassiveDefaultTemplate.groovy</file>
		<file>scripts/templates/active/ActiveDefaultTemplate.groovy</file>
		<file>scripts/templates/authentication/AuthenticationDefaultTemplate.groovy</file>
		<file>scripts/templates/extender/ExtenderDefaultTemplate.groovy</file>
		<file>scripts/templates/httpfuzzerprocessor/FuzzerHttpProcessorDefaultTemplate.groovy</file>
		<file>scripts/templates/httpsender/HttpSenderDefaultTemplate.groovy</file>
		<file>scripts/templates/payloadgenerator/PayloadGeneratorDefaultTemplate.groovy</file>
		<file>scripts/templates/payloadprocessor/PayloadProcessorDefaultTemplate.groovy</file>
		<file>scripts/templates/websocketsender/WebsockerSenderDefaultTemplate.groovy</file>
