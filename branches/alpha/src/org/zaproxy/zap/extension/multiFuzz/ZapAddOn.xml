<zapaddon>
	<name>MultiFuzz</name>
	<version>5</version>
	<description>Provides multi-fuzzing capabilities and their HTTP implementations.</description>
	<author>ZAP Dev Team</author>
	<url></url>
	<changes>Fixes an error that occurred when adding file payloads.</changes>
	<dependson/>
	<extensions>
	    <extension>org.zaproxy.zap.extension.multiFuzz.ExtensionFuzz</extension>
	    <extension>org.zaproxy.zap.extension.multiFuzz.impl.http.ExtensionRegister</extension>
	</extensions>
	<ascanrules/>
	<pscanrules/>
	<filters/>
	<files>
	    <file>scripts/templates/fuzz/FuzzScriptExample.js</file>
	    <file>scripts/templates/payload/PayloadScriptExample.js</file>
	</files>
	<not-before-version>2.3.0</not-before-version>
	<not-from-version>2.4.0</not-from-version>
</zapaddon>