<zapaddon>
	<name>Active scanner rules (beta)</name>
	<version>12</version>
	<description>The beta quality Active Scanner rules</description>
	<author>ZAP Dev Team</author>
	<url/>
	<changes>
	<![CDATA[
	Fixed a ClassNotFoundException while installing the add-on.<br>
	Removed the scanner "Server Side Code Injection Plugin" (promoted to "Active scanner rules" add-on).<br>
	Changed the "LDAP Injection" scanner to scan the parameters defined in the "Active Scan" options.
	]]>
	</changes>
	<dependson/>
	<extensions>
	</extensions>
	<ascanrules>
		<ascanrule>org.zaproxy.zap.extension.ascanrulesBeta.CommandInjectionPlugin</ascanrule>
		<ascanrule>org.zaproxy.zap.extension.ascanrulesBeta.Csrftokenscan</ascanrule>
		<ascanrule>org.zaproxy.zap.extension.ascanrulesBeta.HPP</ascanrule>
		<ascanrule>org.zaproxy.zap.extension.ascanrulesBeta.LDAPInjection</ascanrule>
		<ascanrule>org.zaproxy.zap.extension.ascanrulesBeta.SessionFixation</ascanrule>
		<ascanrule>org.zaproxy.zap.extension.ascanrulesBeta.SQLInjectionHypersonic</ascanrule>
		<ascanrule>org.zaproxy.zap.extension.ascanrulesBeta.SQLInjectionMySQL</ascanrule>
		<ascanrule>org.zaproxy.zap.extension.ascanrulesBeta.SQLInjectionOracle</ascanrule>
		<ascanrule>org.zaproxy.zap.extension.ascanrulesBeta.SQLInjectionPostgresql</ascanrule>
		<ascanrule>org.zaproxy.zap.extension.ascanrulesBeta.TestPersistentXSSAttack</ascanrule>
		<ascanrule>org.zaproxy.zap.extension.ascanrulesBeta.TestPersistentXSSPrime</ascanrule>
		<ascanrule>org.zaproxy.zap.extension.ascanrulesBeta.TestPersistentXSSSpider</ascanrule>
		<ascanrule>org.zaproxy.zap.extension.ascanrulesBeta.UsernameEnumeration</ascanrule>
		<ascanrule>org.zaproxy.zap.extension.ascanrulesBeta.XpathInjectionPlugin</ascanrule>                
	</ascanrules>
	<pscanrules/>
	<filters/>
	<files/>
	<not-before-version>2.2.0</not-before-version>
	<not-from-version/>
</zapaddon>
